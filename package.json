{
  "name": "dreamfactory-mcp",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "bin": {
    "dreamfactory": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "start": "node build/index.js",
    "proxy": "npx mcp-proxy --port 8080 --host 0.0.0.0 --shell node build/index.js",
    "proxy:debug": "npx mcp-proxy --port 8080 --host 0.0.0.0 --debug --shell node build/index.js",
    "proxy:sse": "npx mcp-proxy --port 8080 --server sse --shell node build/index.js",
    "proxy:stream": "npx mcp-proxy --port 8080 --server stream --shell node build/index.js"
  },
  "files": [
    "build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.0",
    "mcp-proxy": "^5.4.0",
    "zod": "^3.0.0"
  }
}
